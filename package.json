{
  "name": "desafio-backend-eduzz",
  "version": "1.0.0",
  "description": "Teste de dev backend pleno da empresa Eduzz",
  "main": "index.js",
  "scripts": {
    "dev:server": "tsx watch src/server.ts",
    "dev:queue": "tsx watch src/runner.ts",
    "dev": "run-s dev:server dev:queue",
    "start:server": "node build/server.js",
    "start:queue": "node build/runner.js",
    "start": "run-s start:server start:queue",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:watch": "vitest --dir src/use-cases",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/bull": "3.15.9",
    "@types/node": "22.0.0",
    "@types/node-cron": "3.0.11",
    "@types/supertest": "6.0.2",
    "eslint": "8.57.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.17.0",
    "supertest": "7.0.0",
    "tsup": "8.2.3",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4"
  },
  "dependencies": {
    "@bull-board/api": "5.21.4",
    "@bull-board/fastify": "5.21.4",
    "@fastify/jwt": "8.0.1",
    "@prisma/client": "5.17.0",
    "@sendgrid/mail": "8.1.3",
    "bcryptjs": "2.4.3",
    "bull": "4.16.2",
    "dayjs": "1.11.12",
    "dotenv": "16.4.5",
    "fastify": "4.28.1",
    "node-cron": "3.0.3",
    "zod": "3.23.8"
  }
}
